using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

namespace HealthCheck.Services
{
    public interface IBar
    {
        int Add(int a, int b);
    }

    public class Bar : IBar
    {
        public int Add(int a, int b)
        {
            if(a > b)
                throw new HttpException();
            return a + b;
        }
    }
}